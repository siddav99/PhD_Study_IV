isbn = {1475-5785},
journal = {Injury Prevention},
language = {English},
mendeley-groups = {Sample,Final List Paper 1},
pages = {4},
pmid = {31273029},
title = {{Mortality and health-related quality of life following injuries and associated factors: a cohort study in Chandigarh, North India}},
