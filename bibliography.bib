number = {3},
pages = {347--351},
publisher = {Elsevier Ltd},
title = {{Predicting Employment Status of Injured Workers Following a Case Management Intervention}},
url = {https://doi.org/10.1016/j.shaw.2017.11.001},
volume = {9},
year = {2018}
