mendeley-groups = {Final List Paper 1},
number = {1},
pages = {97--104},
pmid = {28426563},
title = {{Routine inclusion of long-term functional and patient-reported outcomes into trauma registries: The FORTE project}},
url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=medc1&AN=28426563http://pmt-eu.hosted.exlibrisgroup.com/openurl/46KIB_INST/46KIB_INST_services_page?sid=OVID:medline&id=pmid:28426563&id=doi:10.1097%2FTA.0000000000001490&issn=2163-0755&},
